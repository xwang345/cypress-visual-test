{
  "name": "cypress-visual-test",
  "version": "1.0.0",
  "description": "visual regression testing using Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:percy": "percy exec -- cypress run",
    "cy:test": "cypress run",
    "cy:update-snapshots": "cypress run --env updateSnapshots=true",
    "cy:run-tests:chrome": "cypress run --browser chrome",
    "cy:run-tests:firefox": "cypress run --browser firefox",
    "cy:run-tests:edge": "cypress run --browser edge",
    "cy:run-tests:electron": "cypress run --browser electron",
    "cy:run-tests:all": "cypress run --browser all",
    "cy:run-tests:all:headless": "cypress run --browser all --headless",
    "cy:run-tests:chrome:headless": "cypress run --browser chrome --headless",
    "cy:run-tests:firefox:headless": "cypress run --browser firefox --headless",
    "cy:run-tests:edge:headless": "cypress run --browser edge --headless",
    "cy:run-tests:electron:headless": "cypress run --browser electron --headless",
    "cy:run-tests:chrome:record": "cypress run --browser chrome --record",
    "cy:run-tests:firefox:record": "cypress run --browser firefox --record",
    "cy:run-tests:edge:record": "cypress run --browser edge --record",
    "cy:run-tests:electron:record": "cypress run --browser electron --record",
    "cy:run-tests:all:record": "cypress run --browser all --record",
    "cy:run-tests:all:headless:record": "cypress run --browser all --headless --record",
    "cy:run-tests:chrome:headless:record": "cypress run --browser chrome --headless --record",
    "cy:run-tests:firefox:headless:record": "cypress run --browser firefox --headless --record",
    "cy:run-tests:edge:headless:record": "cypress run --browser edge --headless --record",
    "cy:run-tests:electron:headless:record": "cypress run --browser electron --headless --record",
    "cy:run-tests:chrome:headless:record:ci": "cypress run --browser chrome --headless --record --ci",
    "cy:run-tests:firefox:headless:record:ci": "cypress run --browser firefox --headless --record --ci",
    "cy:run-tests:edge:headless:record:ci": "cypress run --browser edge --headless --record --ci",
    "cy:run-tests:electron:headless:record:ci": "cypress run --browser electron --headless --record --ci",
    "cy:run-tests:all:headless:record:ci": "cypress run --browser all --headless --record --ci",
    "cy:run-dashboard": "cypress run --record --key 3ec48c5a-4ed9-4533-86f9-030b49e7fac3",
    "allure:report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "allure:clear_results": "(if exist allure-results (rmdir /S /Q allure-results))",
    "allure:clear_report": "  (if exist allure-report (rmdir  /S /Q allure-report))",
    "precy-run": "yarn run allure:clear_results && yarn run allure:clear_report",
    "ui-regression": "cypress run --browser chrome --headless --spec cypress/e2e/**.cy.ts --env allure=true --reporter mocha-allure-reporter",
    "cy-run": "cypress run --env allure=true",
    "postcy-run": "yarn run allure:report",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwang345/cypress-visual-test.git"
  },
  "keywords": [
    "cypress",
    "visual",
    "testing",
    "snapshots"
  ],
  "author": "Xiaochen Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xwang345/cypress-visual-test/issues"
  },
  "homepage": "https://github.com/xwang345/cypress-visual-test#readme",
  "dependencies": {
    "@types/cypress-image-snapshot": "^3.1.9",
    "cypress": "^13.7.3",
    "cypress-img-snapshot": "^1.0.0",
    "cypress-plugin-steps": "^1.1.1",
    "cypress-real-events": "^1.11.0",
    "express": "^4.19.2",
    "postinstall": "^0.8.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@percy/cli": "^1.27.5",
    "@percy/cypress": "^3.1.2",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "@simonsmith/cypress-image-snapshot": "^9.0.1",
    "allure-commandline": "^2.28.0",
    "cypress-delete-downloads-folder": "^0.0.4",
    "cypress-split": "^1.23.1",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "pdf-parse": "^1.1.1",
    "ts-node": "^10.9.2"
  }
}
